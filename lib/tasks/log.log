public class Untitled {
  
  public string recur(String question, String answer) {
    if (question.length() == 1) {
        return answer + question + " ";
    }
    for (i = 0; i < question.length(); i++) {
      answer += question.charAt(i);
      subQuestion = question.substring(0, i);
      subQuestion += question.substring(i+1, question.length());
      answer += recur(subQuestion, answer);
    }
  }
  
  public static string answer(String question) {
    return recur(question);
  }
}

