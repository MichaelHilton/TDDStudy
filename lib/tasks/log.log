            if(isStrike(throwNumber)){
                points += 10 + strikeBonus(throwNumber);
            }
            else if(isSpare(throwNumber)){
                points += 10 + spareBonus(throwNumber);
            }
            else {
                points += framePoints(throwNumber);
            }
            frameNumber++;
        }

        return points;
    }

    private boolean isSpare(throwNumber) {
        return pinsKnockedDownOnThrow[throwNumber] + pinsKnockedDownOnThrow[throwNumber + 1] == 10;
    }

