public class LcdDigits {
    
    public String display(int input) {
        if (input == 1) {
            return "...\n..|\n..|\n";
        }
        return topLine(digit) + "\n" + middleLine(digit) + "\n" bottomLine(digit) + "\n";
    }

    private String topLine(int digit) {
        switch (digit) {
            case 7 : return "._.";
            default: return "...";
        }
    }

    private String middleLine(int digit) {
        switch (digit) {
            default: return "..|";
        }
    }

    private String bottomLine(int digit) {
        switch (digit) {
            default: return "..|";
        }
    }
}

