public class PrimeFactor {
    
    public static int[] getFactors(int i) {
        if (i <= 0) {
            throw new IllegalArgumentException("argument must be greater than 0");
        }
        int[] result = new int[i];
        if (i == 1) {
            result[0] = 1;
        } else {      
            int currentNumber = i;
            int idx = 0;
            for (int d = 2; d <= i; d++) {
                while (currentNumber % d == 0) {
                    result[idx++] = d;
                    currentNumber = currentNumber / d;
                }
            }     
        }
        return result;
    }
}

