public class Yatzy {
    
    public static int calculateChance(int [] dice) {
        
        int sum =0;

        for(int i:dice) {
            sum +=i;
        }

        return sum;
    }

    public static int calculateYatzy(int [] dice) {
        
        int currentDie =dice[0];

        for(int i:dice) {
            if(i!=currentDie) {
                return 0;
            }
        }

        return 50;
    }

    public static int calculatePair(int [] dice) {
        
        int bestSum = 0;

        for(int i=0; i<4; i++) {
            for(int j=i+1; j<4; j++) {
                if(dice[i]==dice[j]) {
                    if (dice[i]+dice[j] > bestSum) {
                        bestSum = dice[i]+dice[j];
                    }
                }
            }
        }

        return bestSum;
    }

}

